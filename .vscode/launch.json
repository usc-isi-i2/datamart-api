    "version": "0.2.0",
    "configurations": [
        {
            "name": "API Backend",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--port",
                "14000"
            ],
            "jinja": true
        },
        {
            "name": "Clean for Causex",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/script/clean-for-causex.py",
            "args": [
                "d:\\temp\\datamart\\causex-vars.xlsx"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Create Fuzzy-search Views",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/script/create_search_views.py",
            "args": [
                "--recreate"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Import kgtk edges",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/script/import_tsv_postgres.py",
            "args": [
                "c:\\users\\zmbq\\Downloads\\kgtk.tsv"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
    ]
}